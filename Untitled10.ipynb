   "source": [
    "import numpy as np\n",
    "x = np.array([[-5,2],[3,-4],[2,4],[2,-0],[0,0]])\n",
    "sigma = np.cov(x[:,0],x[:,1])\n",
    "\n",
    "from numpy import linalg as LA\n",
    "eignVal, eignVec = LA.eig(sigma)\n",
    "print(np.around(eignVec))"
   ]
  }
